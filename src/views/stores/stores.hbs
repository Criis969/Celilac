<div class="container-md p-2">
    <div class="list-group">
        {{#each stores}}
        <a href="{{google_link}}" class="list-group-item list-group-item-action" target="_blank">
            <div class="d-flex w-100 justify-content-between">
                <h5 class="mb-1">{{store_name}}</h5>
                <small>{{city}}</small>
            </div>
            <p class="mb-1">{{store_type}}</p>
        </a>
        {{/each}}
    </div>
</div>